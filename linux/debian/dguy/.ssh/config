# ~/.ssh/config
# Repositories
Host github.com
    IdentityFile ~/.ssh/github
Host github
    HostName github.com
    User git
    IdentityFile ~/.ssh/github
Host githubclem
    HostName github.com
    User git
    IdentityFile ~/.ssh/clemsonStuff/github

Host codeberg.com
    IdentityFile ~/.ssh/codeberg
Host codeberg
    HostName codeberg.org
    User git
    IdentityFile ~/.ssh/codeberg

