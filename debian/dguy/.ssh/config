# ~/.ssh/config
Host *
    IdentityAgent none
    IdentitiesOnly yes
Host 10.3.*.*
    User ansible
    StrictHostKeyChecking no
    UserKnownHostsFile /dev/null
    IdentityFile ~/.ssh/clemsonStuff/ansible

Host github.com
    IdentityFile ~/.ssh/github
Host github
    HostName github.com
    User git
    IdentityFile ~/.ssh/github
Host codeberg.com
    IdentityFile ~/.ssh/codeberg
Host codeberg
    HostName codeberg.org
    User git
    IdentityFile ~/.ssh/codeberg

# Personal
